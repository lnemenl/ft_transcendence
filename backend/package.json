  "keywords": [],
  "author": "",
  "license": "ISC",
  "workspaces": [
    "src/*"
  ],
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^20.14.2",
    "@typescript-eslint/eslint-plugin": "^8.0.0",
    "@typescript-eslint/parser": "^8.0.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^9.1.2",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^16.4.0",
    "pino-pretty": "^13.1.1",
    "prettier": "^3.3.2",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "typescript-eslint": "^8.45.0"
  },
  "dependencies": {
    "@fastify/cors": "^11.1.0",
    "@prisma/adapter-better-sqlite3": "^6.16.3",
    "@prisma/client": "^6.16.3",
    "bcrypt": "^6.0.0",
    "dotenv": "^17.2.3",
    "fastify": "^5.6.1",
    "jsonwebtoken": "^9.0.2",
    "npm": "^11.6.1",
    "prisma": "^6.16.3"
  }
