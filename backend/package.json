  "description": "The ft_transcendence backend monorepo",
  "type": "module",
  "private": true,
  "scripts": {
    "lint": "eslint \"src/**/*.ts\"",
    "format": "prettier --write \"src/**/*.ts\"",
    "check": "prettier --check \"src/**/*.ts\"",
    "prisma": "npx prisma generate --schema=./prisma/schema.prisma",
    "dev": "npx ts-node src/index.ts",
    "build": "tsc --project tsconfig.json",
    "start": "node dist/src/src/index.js"
